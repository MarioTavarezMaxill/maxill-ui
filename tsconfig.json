{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"allowUnreachableCode": false,
		"declaration": false,
		"experimentalDecorators": true,
		"lib": ["dom", "es2017"],
		"moduleResolution": "node",
		"module": "esnext",
		"target": "es2017",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"jsx": "react",
		"jsxFactory": "h"
	},
	"include": ["src"],
	"baseUrl": "./src",
	"paths": {
		"@assets/*": ["assets/*"]
	},
	"exclude": ["node_modules"]
}
