{
	"name": "maxill-ui",
	"version": "0.0.4",
	"repository": {
		"type": "git",
		"url": "https://github.com/MarioTavarezMaxill/maxill-ui"
	},
	"description": "Maxill UI Component Library",
	"main": "dist/index.cjs.js",
	"module": "dist/index.js",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"types": "dist/types/index.d.ts",
	"collection": "dist/collection/collection-manifest.json",
	"collection:main": "dist/collection/index.js",
	"unpkg": "dist/maxill-ui/maxill-ui.esm.js",
	"files": [
		"dist/",
		"loader/"
	],
	"scripts": {
		"test": "stencil test --spec --coverage",
		"test-e2e": "stencil test --spec --e2e",
		"test-e2e.watch": "stencil test --spec --e2e --coverage --watchAll",
		"test.watch": "stencil test --spec --coverage --watchAll",
		"build": "stencil build --docs",
		"start": "stencil build --dev --watch --serve",
		"generate": "stencil generate",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"prepare": "husky install",
		"lint": "eslint '*/**/*.{js,ts,tsx}'",
		"prettier-write": "prettier --write",
		"prettier-check": "prettier --check",
		"release": "standard-version --releaseCommitMessageFormat=\"chore(release): {{currentTag}} [ci skip]\"",
		"release:pre": "standard-version --prerelease --releaseCommitMessageFormat=\"chore(release): {{currentTag}} [ci skip]\"",
		"release:alpha": "standard-version --prerelease alpha"
	},
	"dependencies": {
		"@stencil/core": "^2.13.0",
		"@types/npm": "^7.19.0",
		"bootstrap": "^5.3.0-alpha1"
	},
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@stencil/angular-output-target": "^0.4.0",
		"@stencil/sass": "^2.0.1",
		"@storybook/addon-actions": "^6.5.15",
		"@storybook/addon-essentials": "^6.5.15",
		"@storybook/addon-interactions": "^6.5.15",
		"@storybook/addon-knobs": "^6.4.0",
		"@storybook/addon-links": "^6.5.15",
		"@storybook/addon-notes": "^5.3.21",
		"@storybook/builder-webpack4": "^6.5.15",
		"@storybook/html": "^6.5.15",
		"@storybook/manager-webpack4": "^6.5.15",
		"@storybook/testing-library": "^0.0.13",
		"@types/jest": "^27.5.2",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"babel-loader": "^8.3.0",
		"chromatic": "^6.14.0",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.2",
		"jest": "^27.5.1",
		"jest-cli": "^27.5.1",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.1",
		"prettier-eslint": "^15.0.1",
		"puppeteer": "^10.0.0",
		"standard-version": "^9.5.0",
		"stencil-inline-svg": "^1.1.0",
		"storybook-addon-stencil": "^0.1.14",
		"webpack-merge": "^5.8.0"
	},
	"license": "MIT"
}
